#ifndef HAL_SWITCH__H_
#define HAL_SWITCH__H_

void SWITCH_per();
void SWITCH_Ini();

#include "SWITCH.h"

struct Button{
	//uint32_t Read;
	uint32_t Set;
	uint32_t Pull;
};

/* DEFINE GPIO PUPDR */
#define NO_PULL		00
#define PULL_UP		01
#define PULL_DOWN	10
#define RESERVED	11

/* DEFINE GPIO MODER */
#define INPUT		00
#define OUTPUT		01
#define ALTERNATE	10
#define ANALOG		11

#endif /* HAL_SWITCH__H_ */
